    "def count_vowels():\n",
    "    user_string = input(\"Enter a string: \")\n",
    "    vowels = \"aeiouAEIOU\"\n",
    "    count = sum(1 for char in user_string if char in vowels)\n",
    "    print(\"The number of vowels is:\", count)"
   ]
