   "source": [
    " def recursive_factorial(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    return n * recursive_factorial(n - 1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "ddc41de3-b88e-4474-86b3-9be6b642a2c3",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The factorial of 5 is 120\n"
     ]
    }
   ],
   "source": [
    "n = 5\n",
    "print(f\"The factorial of {n} is {recursive_factorial(n)}\")"
   ]
