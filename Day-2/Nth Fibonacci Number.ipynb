   "source": [
    "def nth_fibonacci_number():\n",
    "    n = int(input(\"Enter a term number: \"))\n",
    "    a, b = 0, 1\n",
    "    for _ in range(n - 1):\n",
    "        a, b = b, a + b\n",
    "    print(f\"The {n}th Fibonacci number is {a}.\")"
   ]
